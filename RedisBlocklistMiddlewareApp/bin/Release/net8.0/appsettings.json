{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "StackExchange.Redis": "Information",
      "RedisBlocklistMiddlewareApp": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RedisConnection": "localhost:6379"
  },
  "Redis": {
    "BlocklistKeyPrefix": "blocklist:",
    "DbBlocklist": 2
  },
  "Heuristics": {
    "KnownBadUaSubstrings": [
      "GPTBot",
      "CCBot",
      "ClaudeBot",
      "Bytespider",
      "PetalBot",
      "AhrefsBot",
      "SemrushBot",
      "MJ12bot",
      "DotBot",
      "Scrapy",
      "python-requests",
      "curl",
      "wget",
      "masscan",
      "zgrab",
      "nmap",
      "sqlmap",
      "nikto"
    ],
    "TarpitRewritePath": "/anti-scrape-tarpit/",
    "CheckEmptyUa": true,
    "CheckMissingAcceptLanguage": true,
    "CheckGenericAccept": true
  }
}
